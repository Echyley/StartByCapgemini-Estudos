algoritmo "Repita - Exercitando (Menu)"

var
opcaomenu: literal
totalconta: inteiro
inicio
totalconta <- 0
repita
  escreval ("1 - Troca de óleo.")
  escreval ("2 - Balanceamento.")
  escreval ("3 - Sair.")
  leia (opcaomenu)
  se opcaomenu = "1" entao
     totalconta <- totalconta + 100
  senao
  se opcaomenu = "2" entao
     totalconta <- totalconta + 70
  fimse
  fimse
limpatela
ate opcaomenu = "3"
escreval ("Total conta: R$", totalconta)
fimalgoritmo

