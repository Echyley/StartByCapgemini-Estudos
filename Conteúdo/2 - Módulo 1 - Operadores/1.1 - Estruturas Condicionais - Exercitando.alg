algoritmo "Estruturas Condicionais - Exercitando"

var
veloveiculo, veloleve, velomedia, velograve, velogravissima: real
  
inicio
veloleve <- 50* 1.1
velomedia <- 70
velograve <- 90
velogravissima <- 110
Escreval ("Qual foi sua velo?")
Leia (veloveiculo)

