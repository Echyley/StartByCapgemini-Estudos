algoritmo "Extra: ^Chapéu^"
//O sinal "^" é nosso operador para elevado.
var
altura, peso, imc: real
inicio
Escreval ("Informe sua altura.")
Leia (altura)
Escreval ("Informe seu peso.")
Leia (peso)
imc <- peso/altura ^ 2


se imc < 19 entao
   escreval ("Abaixo do peso :(")
senao
    se imc < 24 entao
       escreval ("Peso Ideal :D")
senao
   escreval ("Acima do peso :(")
    fimse
fimse
Escreval ("Este é o seu imc: ")
Escreval (imc)
fimalgoritmo

