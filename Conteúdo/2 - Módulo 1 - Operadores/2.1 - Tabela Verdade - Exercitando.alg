algoritmo "Tabela Verdade - Exercitando"
//TABELA VERDADE PARA "=":
   //nao & sim = NAO
   //sim & nao = NAO
   // sim & sim = SIM
   // nao & nao = NAO
// PARA "ou":
   //nao ou sim = SIM
   //sim ou nao = SIM
   // sim ou sim = SIM
   // nao ou nao = NAO
var
lado1, lado2, lado3: real
inicio
Escreval ("Lado 1")
Leia (lado1)
Escreval ("Lado 2")
Leia (lado2)
Escreval ("Lado 3")
Leia (lado3)

se (lado1 > lado2 + lado3) ou (lado2 > lado1 + lado3) ou (lado3 > lado1 + lado2) entao
    Escreval ("Não é triangulo")
senao
    se  (lado1 = lado2) e (lado2 = lado3) entao
         Escreval ("Equilátero!")
    senao
    se (lado1 = lado2) ou (lado2 = lado3) ou (lado1 = lado3) entao
        Escreval ("Isósceles")
    senao
        Escreval ("Escaleno")
    fimse
    fimse
fimse


fimalgoritmo



