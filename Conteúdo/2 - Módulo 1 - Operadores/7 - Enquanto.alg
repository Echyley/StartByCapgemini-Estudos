algoritmo "Enquanto I"
//O operador Enquanto ou "while" pode calcular determinada situação
//automaticamente em loop, se não houver uma saída o while continuará
//infinitamente.
var
alturamarcelo, alturajoao, crescAnualmarcelo, crescAnualjoao: real
idade: inteiro
inicio
alturamarcelo <- 1.20
alturajoao <- 1.05
crescAnualmarcelo <- 0.05
crescAnualjoao <- 0.07
idade <- 8

enquanto alturamarcelo >= alturajoao faca
   alturamarcelo <- alturamarcelo + crescAnualmarcelo
   alturajoao <- alturajoao + crescAnualjoao
   idade <- idade + 1
fimenquanto
Escreval ("Idade ", idade)
Escreval ("Altura João ", alturajoao)
EScreval ("Altura Marcelo ", alturamarcelo)

fimalgoritmo

