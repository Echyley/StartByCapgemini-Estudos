algoritmo "Tabela Verdade"
//TABELA VERDADE PARA "=":
   //nao & sim = NAO
   //sim & nao = NAO
   // sim & sim = SIM
   // nao & nao = NAO
// PARA "ou":
   //nao ou sim = SIM
   //sim ou nao = SIM
   // sim ou sim = SIM
   // nao ou nao = NAO
var
 i, j: real

inicio
Escreval ("Qual o valor de i?")
Leia (i)
Escreval ("Qual o valor de j?")
Leia (j)

se (i = 10) e (j = 20) entao
    Escreval ("Fazer algo")
senao
    Escreval ("...")
fimse
fimalgoritmo



