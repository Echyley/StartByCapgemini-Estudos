algoritmo "Vetores (Arrays) - Exercitando - Busca"
//É como um baú capaz de armazenar e executar em apenas uma variável várias outras,
//quantas quiser ou precisar.
var
nomes: vetor[1..5] de literal
pesquisa: literal
i, sucesso: inteiro
inicio
para i de 1 ate 5 faca
  Escreval ("Informe o nome de cadastro.")
  leia(nomes[i])
fimpara
limpatela

escreval ("Informe um nome a pesquisar.")
leia (pesquisa)

sucesso <- 0

para i de 1 ate 5 faca
  se nomes [i] = pesquisa entao
    sucesso <- 1
  fimse
fimpara

se sucesso = 1 entao
  escreval ("Cliente encontrado :D")
  senao
    escreval ("Cliente não encontrado :(")
fimse
fimalgoritmo


